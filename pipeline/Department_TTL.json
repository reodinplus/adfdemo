{
	"name": "Department_TTL",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "DS_REST_QYWeChat",
						"type": "DatasetReference",
						"parameters": {
							"RelativeURL": {
								"value": "department/list?access_token=@{pipeline().parameters.access_token}&id=",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "asql_dataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"access_token": {
				"type": "string"
			}
		},
		"folder": {
			"name": "QyWeChat/TTL"
		},
		"annotations": []
	}
}